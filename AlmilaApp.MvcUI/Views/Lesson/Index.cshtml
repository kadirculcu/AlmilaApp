@model AlmilaApp.Entities.ViewModel.LessonsViewModel
@{
    ViewData["Title"] = "Index";
}

<h1>Dersler</h1>
<table class="table">
    <thead>
        <tr>
            <th>Ders ID</th>
            <th>Ders Adı</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Lesson)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
       
            </tr>
        }
    </tbody>

</table>
